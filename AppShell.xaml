<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Leonardo_Andrade_PROYECTOAPUNTES.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Leonardo_Andrade_PROYECTOAPUNTES"
    Shell.FlyoutBehavior="Disabled"
    Title="Leonardo_Andrade_PROYECTOAPUNTES">


    <TabBar>
        <ShellContent
            Title="Notes"
            ContentTemplate="{DataTemplate local:LAqNotePage}"
            Icon="{OnPlatform 'icon_notes.png', iOS='icon_notes_ios.png', MacCatalyst='icon_notes_ios.png'}" />

        <ShellContent
            Title="About"
            ContentTemplate="{DataTemplate local:LAqAboutPage}"
            Icon="{OnPlatform 'icon_about.png', iOS='icon_about_ios.png', MacCatalyst='icon_about_ios.png'}" />
    </TabBar>

</Shell>